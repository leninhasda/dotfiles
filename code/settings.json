    ,
    // "workbench.fontAliasing": "antialiased",
    "git.inputValidationSubjectLength": null,
    "git.inputValidationLength": 120,
    "editor.trimAutoWhitespace": false,
    "files.trimTrailingWhitespace": false,
    "editor.useTabStops": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.rulers": [
        80,
        120
    ],
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#505050",
        "[Gruvbox Material Dark]": {
            "sideBar.border": "#595353",
            // "sideBar.border": "#72c2ff",
        }
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    // "python.showStartPage": false,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "git.enableSmartCommit": true,
    // "todo-tree.tree.showScanModeButton": false,
    // deprecated
    // "terminal.integrated.shellArgs.osx": [
    //     "-c",
    //     "if [ -e .env ]; then export $(cat .env | xargs); fi; zsh;"
    // ],
    "terminal.integrated.tabs.enabled": true,
    "markdown.marp.enableHtml": true,
    "workbench.editorAssociations": {
        "*.png": "luna.editor",
        "*.ipynb": "jupyter-notebook",
        "*.plist": "default"
    },
    "editor.tabSize": 2,
    "editor.cursorStyle": "line",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "security.workspace.trust.untrustedFiles": "open",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "notebook.lineNumbers": "on",
    "search.showLineNumbers": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[proto]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "markdown.extension.list.indentationSize": "inherit",
    "[sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "go.toolsManagement.autoUpdate": true,
    // "bracketPairColorizer.depreciation-notice": false,
    // "kite.showWelcomeNotificationOnStartup": false,
    "markdown.styles": [
        "https://use.fontawesome.com/releases/v5.7.1/css/all.css"
    ],
    "editor.tokenColorCustomizations": {
        // "[GitHub Dark Dimmed]": {
        "[*]": {
            "textMateRules": [
                {
                    "scope": "keyword.function.go",
                    "settings": {
                        "fontStyle": "italic"
                    }
                },
                // {
                //     "scope": "comment.line.double-slash.go",
                //     "settings": {
                //         "fontStyle": "italic"
                //     }
                // },
                {
                    "scope": "markup.inline.raw.string.markdown",
                    "settings": {
                        "foreground": "#93d56a",
                        // "fontStyle": "bold"
                    }
                },
                {
                    "scope": "punctuation.definition",
                    "settings": {
                        "foreground": "#d7b94d",
                        // "fontStyle": "bold"
                    }
                }, 
                {
                    "scope": "markup.quote.markdown",
                    "settings": {
                        "foreground": "#d8d198",
                        "fontStyle": "italic"
                    }
                },
                {
                    // "scope": "meta.structure.dictionary.value.json.comments",
                    "scope": "support.type.property-name.json.comments",
                    "settings": {
                        // "foreground": "#99C794"
                        // "foreground": "#c09924"
                    }
                },
                {
                    "scope": "string.quoted.double.json.comments",
                    "settings": {
                        "foreground": "#deb510"
                    }
                }
            ]
        }
    },
    // "rapidapi.terminalLink.enabled": false,
    "redhat.telemetry.enabled": false,
    // vim keybinding fix
    "vim.useCtrlKeys": true,
    "vim.handleKeys": { 
        "<C-w>": false
        , "<C-j>": false 
        , "<C-m>": false
        , "<caps>": false
        , "<C-a>": false
        , "<C-f>": false
        , "<C-c>": false
        , "<C-v>": false
        , "<C-x>": false
    },
    "workbench.editor.untitled.hint": "hidden",
    "[python]": {
        "editor.formatOnType": true
    },
    "explorer.confirmDelete": false,
    "editor.minimap.enabled": false
}